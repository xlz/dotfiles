{
    "breadcrumbs.enabled": false,
    "editor.acceptSuggestionOnCommitCharacter": false,
    "editor.acceptSuggestionOnEnter": "off",
    "editor.autoClosingBrackets": "never",
    "editor.autoClosingOvertype": "never",
    "editor.autoClosingQuotes": "never",
    "editor.detectIndentation": false,
    "editor.dragAndDrop": false,
    "editor.emptySelectionClipboard": false,
    "editor.fastScrollSensitivity": 1,
    "editor.folding": false,
    "editor.fontFamily": "'monospace', monospace, 'Droid Sans Fallback'",
    "editor.glyphMargin": false,
    "editor.highlightActiveIndentGuide": false,
    "editor.lightbulb.enabled": false,
    "editor.links": false,
    "editor.minimap.enabled": false,
    "editor.overviewRulerBorder": false,
    "editor.parameterHints.enabled": false,
    "editor.quickSuggestions": {
        "other": false,
        "comments": false,
        "strings": false
    },
    "editor.renderFinalNewline": false,
    "editor.renderIndentGuides": false,
    "editor.renderLineHighlight": "all",
    "editor.roundedSelection": false,
    "editor.scrollBeyondLastLine": false,
    "editor.snippetSuggestions": "none",
    "editor.suggestOnTriggerCharacters": false,
    "editor.wordBasedSuggestions": false,
    "editor.autoSurround": "never",
    "editor.tabSize": 2,
    "extensions.ignoreRecommendations": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false,
    "update.mode": "none",
    "update.showReleaseNotes": false,
    "window.titleBarStyle": "custom",
    "workbench.activityBar.visible": false,
    "workbench.colorTheme": "Solarized Dark",
    "workbench.editor.enablePreview": false,
    "workbench.editor.enablePreviewFromQuickOpen": false,
    "workbench.editor.showIcons": false,
    "workbench.enableExperiments": false,
    "workbench.list.automaticKeyboardNavigation": false,
    "workbench.startupEditor": "newUntitledFile",
    "workbench.tips.enabled": false,
    "workbench.tree.indent": 16,
    "workbench.tree.renderIndentGuides": "none",
    "C_Cpp.autocomplete": "Disabled",
    "C_Cpp.clang_format_fallbackStyle": "{ BasedOnStyle: Google, AllowShortIfStatementsOnASingleLine: false, AllowShortLoopsOnASingleLine: false }",
    "C_Cpp.clang_format_path": "/usr/bin/clang-format",
    "C_Cpp.default.cStandard": "c99",
    "C_Cpp.default.cppStandard": "c++14",
    "C_Cpp.default.intelliSenseMode": "gcc-x64"
}
